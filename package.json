{
  "private": true,
  "packageManager": "pnpm@8.5.0",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm run -C play dev",
    "docs:dev": "pnpm run -C docs dev",
    "build": "node ./script/build",
    "buildes": "rimraf dist1 && node ./script/build-es.js",
    "build:style": "pnpm run -C packages/styles build",
    "clean:lib": "rimraf dist"
  },
  "dependencies": {
    "@uif-plus/components": "workspace:^",
    "@uif-plus/styles": "workspace:^",
    "vue": "3.3.4"
  },
  "exports": {
    ".": {
      "require": "./dist/uif.umd.js",
      "import": "./dist/uif.mjs"
    },
    "./package.json": "./package.json",
    "./lib/*": "./dist/*"
  },
  "files": [
    "dist",
    "packages"
  ],
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rimraf": "^5.0.5",
    "rollup": "^2.79.1",
    "rollup-plugin-esbuild": "4.9.1",
    "rollup-plugin-vue": "6",
    "stylus": "^0.60.0",
    "unplugin-vue-macros": "0.11.2",
    "vite": "^4.4.11",
    "vite-plugin-externals": "^0.6.2"
  },
  "engines": {
    "node": ">=14.16"
  }
}